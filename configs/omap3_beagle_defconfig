# Architecture
BR2_arm=y
BR2_cortex_a8=y

# Toolchain
BR2_TOOLCHAIN_EXTERNAL=y
# BR2_TOOLCHAIN_EXTERNAL_DOWNLOAD is not set
BR2_TOOLCHAIN_EXTERNAL_CODESOURCERY_ARM201103=y
BR2_TOOLCHAIN_EXTERNAL_PATH="/opt/CodeSourcery"
BR2_TOOLCHAIN_EXTERNAL_PREFIX="arm-none-linux-gnueabi"
BR2_TOOLCHAIN_EXTERNAL_GLIBC=y
# BR2_PREFER_SOFT_FLOAT is not set
# BR2_SOFT_FLOAT is not set
BR2_TARGET_OPTIMIZATION="-pipe -mfpu=neon -ftree-vectorize -mfloat-abi=softfp"

# Serial port
BR2_TARGET_GENERIC_GETTY_PORT="ttyO2"
